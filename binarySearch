#include<iostream>
using namespace std;
int binaryS(int arr[],int n,int key)
{
    int start=0;
    int end=n-1;
    int mid=start+(end-start)/2;
    while(start<=end){
        if(arr[mid]==key)
        {
        return mid;
        }
        else if(key>arr[mid])
        {
            start=mid+1;
        }
        else{
            end=mid-1;
        }
        mid=start+(end-start)/2;
    }
    return -1;
}
int main()
{
    int arr[6]={20,7,9,54,76,3};
    int key;
    cout<<"Enter a Key";
    cin>>key;
    cout<<"Element is present at index:"<<binaryS(arr,6,key);

}
