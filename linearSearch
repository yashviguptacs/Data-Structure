#include<iostream>
using namespace std;
bool linearS(int arr[],int n,int key){
    for(int i=0;i<n;i++)
    {
        if(key==arr[i])
        {
            return 1;
        }
    }
    return 0;
}
int main(){
    int arr[6]={7,9,10,4,45,8};
    int key;
    cout<<"Enter a Element You want to find in given Array"<<" ";
    cin>>key;
    bool found=linearS(arr,6,key);
    if(found==1){
        cout<<"Element is present";
    }
    else{
        cout<<"Element is not present";
    }
return 0;

}
